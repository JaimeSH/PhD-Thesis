@article{Li1999,
abstract = {Recent studies in finance domain suggest that technical analysis may have merit to predictability of$\backslash$nstock. Technical rules are widely used for market assessment and timing. For example, moving average$\backslash$nrules are used to make "buy" or "sell" decisions at each day. In this paper, to explore the potential$\backslash$nprediction power of technical analysis, we present a genetic programming based system FGP (Financial$\backslash$nGenetic Programming), which specialises in taking some well known technical rules and adapting them$\backslash$nto prediction problems. FGP uses the power of genetic programming to generate decision trees through$\backslash$nefficient combination of technical rules with self-adjusted thresholds. The generated rules are more$\backslash$nsuitable for the prediction problem at hand. FGP was tested extensively on historical S{\&}P 500 data$\backslash$nthrough a specific prediction problem. Preliminary results show that it outperforms commonly used,$\backslash$nnon-adaptive, individual technical rules with respect to prediction accuracy and annualised rate of$\backslash$nreturn over two different out-of-sample test periods (three and a half year in each period).},
