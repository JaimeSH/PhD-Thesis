journal = {Journal of Economic Dynamics and Control},
keywords = {Agent-based computational economics,Artificial stock markets,Business school,D83,G12,G14,Genetic programming,Social learning},
mendeley-groups = {ArtificialTrading,A Novel Method Based on Fuzzy Inference Systems Multi-agent Systems and Genetic Programming for the Forecast of Financial Markets,PhD Thesis/Genetic Programming,PhD Thesis/Multi-agent Systems in Finance,Using intuitionistic fuzzy inference systems for the creation of an agent-based financial market model/Multi-agent Systems in Finance},
number = {3-4},
pages = {363--393},
