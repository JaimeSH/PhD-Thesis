mendeley-groups = {A Novel Method Based on Fuzzy Inference Systems Multi-agent Systems and Genetic Programming for the Forecast of Financial Markets,ArtificialTrading,PhD Thesis/Regression},
number = {2},
pages = {115--125},
pmid = {206604507},
publisher = {Elsevier B.V.},
title = {{Financial time series forecasting using independent component analysis and support vector regression}},
