isbn = {1063-6560},
issn = {1063-6560},
journal = {Evolutionary Computation},
mendeley-groups = {PhD Thesis/Genetic Algorithms},
number = {1},
pages = {1--23},
pmid = {741},
title = {{An Overview of Evolutionary Algorithms for Parameter Optimization}},
volume = {1},
year = {2008}
}
